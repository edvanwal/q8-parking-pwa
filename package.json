{
  "name": "q8-parking-pwa",
  "version": "1.1.0",
  "description": "Q8 Liberty Parking PWA - Smart parking management",
  "scripts": {
    "screenshot": "node scripts/screenshot.js",
    "sync": "node scripts/sync-to-public.js",
    "deploy": "npm run sync && firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:functions": "firebase deploy --only functions",
    "functions:install": "cd functions && npm install",
    "functions:logs": "firebase functions:log"
  },
  "dependencies": {
    "firebase-tools": "^13.0.0"
  },
  "devDependencies": {
    "playwright": "^1.49.0"
  },
  "cdnDependencies": {
    "firebase": "10.7.1",
    "google-fonts-mulish": "https://fonts.googleapis.com/css2?family=Mulish:wght@400;600;700;800&display=swap"
  },
  "engines": {
    "node": ">=18"
  }
}
